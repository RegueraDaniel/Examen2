<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>web_empleados</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
		<servlet-name>DepartamentosServlet</servlet-name>
		<servlet-class>org.iesvdm.servlet.DepartamentosServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DepartamentosServlet</servlet-name>
		<url-pattern>/departamentos/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>EmpleadosServlet</servlet-name>
		<servlet-class>org.iesvdm.servlet.EmpleadosServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmpleadosServlet</servlet-name>
		<url-pattern>/empleados/*</url-pattern>
	</servlet-mapping>
  
</web-app>